{
    "name": "lykegenes/laravel-countries",
    "description": "Get informations about countries.",
    "keywords": [
        "laravel",
        "countries"
    ],
    "homepage": "https://github.com/Lykegenes/laravel-countries",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Samson",
            "email": "patricksamson236@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "mledoze/countries": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0|^9.0",
        "mockery/mockery": "1.3.*",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Lykegenes\\LaravelCountries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lykegenes\\LaravelCountries\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "compile-data-source": "php ./scripts/compile-data-source.php"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lykegenes\\LaravelCountries\\ServiceProvider"
            ],
            "aliases": {
                "Countries": "Lykegenes\\LaravelCountries\\Facades\\Countries"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
